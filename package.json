{
  "name": "podkrepi-bg-frontend",
  "version": "0.8.3",
  "license": "MIT",
  "repository": "https://github.com/orgs/podkrepi-bg",
  "scripts": {
    "predev": "shx test -e .env.local && exit 0 || shx echo 'You need to create .env.local file. Please check README.md!' && exit 1",
    "dev": "yarn && next dev -p 3040",
    "build": "next build",
    "start": "next start -p 3040",
    "test": "jest --env=jsdom",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test:watch": "jest --env=jsdom --watch --silent=false",
    "lint": "eslint . --fix --ext=ts,tsx",
    "lint:next": "next lint",
    "lint:styles": "stylelint **.scss",
    "format": "prettier --write .",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "prepare": "husky install",
    "release": "./manifests/release.sh",
    "release:patch": "./manifests/release.sh patch"
  },
  "dependencies": {
    "@emotion/cache": "^11.5.0",
    "@emotion/react": "^11.5.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.5",
    "@mui/lab": "^5.0.0-alpha.52",
    "@mui/material": "^5.0.5",
    "@mui/styles": "^5.0.2",
    "@mui/x-data-grid": "next",
    "@react-keycloak/ssr": "3.3.0",
    "@react-keycloak/web": "3.4.0",
    "@sentry/nextjs": "6.5.1",
    "@supabase/supabase-js": "1.13.1",
    "axios": "0.21.4",
    "classnames": "2.3.1",
    "date-fns": "2.24.0",
    "formik": "2.2.9",
    "highcharts": "9.1.0",
    "highcharts-react-official": "3.0.0",
    "keycloak-js": "^15.0.2",
    "mobx": "6.3.2",
    "mobx-react": "7.2.0",
    "next": "11.1.1",
    "next-csrf": "beta",
    "next-i18next": "8.4.0",
    "nookies": "^2.5.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-gtm-module": "2.0.11",
    "react-in-viewport": "1.0.0-alpha.17",
    "react-query": "^3.23.2",
    "sass": "1.34.1",
    "sharp": "^0.29.0",
    "slugify": "^1.6.0",
    "typewriter-effect": "2.17.0",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/cookie": "^0.4.1",
    "@types/node": "14.14.37",
    "@types/react": "17.0.3",
    "@types/react-gtm-module": "2.0.0",
    "@types/yup": "0.29.11",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "babel-jest": "^27.3.1",
    "eslint": "7.27.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "7.0.1",
    "jest": "^27.3.1",
    "lint-staged": "11.0.0",
    "prettier": "2.3.0",
    "shx": "^0.3.3",
    "stripe": "^8.184.0",
    "stylelint": "13.13.1",
    "stylelint-config-sass-guidelines": "8.0.0",
    "typescript": "4.3.5"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "yarn run lint"
    ],
    "*.{ts,tsx,js,json}": [
      "yarn run format"
    ],
    "*.scss": "yarn run lint:styles"
  }
}
